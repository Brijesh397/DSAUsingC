#include <stdio.h>
#include <stdint.h>

uint32_t printElementsOfAnArray(uint32_t arr[],uint32_t size);

int main()
{

}

uint32_t printElementsOfAnArray(uint32_t arr[],uint32_t size)
{
	if(size == 1)
	{
		return 1;
	}
	printElementsOfAnArray(arr,size-1);
	printf("The elements in the array are : %d",arr[size]);
	return 1;
}
